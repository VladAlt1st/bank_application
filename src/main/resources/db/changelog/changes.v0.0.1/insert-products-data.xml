<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="insert products data 1" author="Vladyslav Rudnytskyi">
        <insert tableName="products">
            <column name="name" value="deposit"/>
            <column name="type" value="DEPOSIT"/>
            <column name="status" value="AVAILABLE"/>
            <column name="currency_code" value="USD"/>
            <column name="interest_rate" value="-3"/>
            <column name="product_limit" value="0"/>
            <column name="created_at" value="2022-10-08"/>
            <column name="updated_at" value="2022-10-08"/>
        </insert>
    </changeSet>

    <changeSet id="insert products data 2" author="Vladyslav Rudnytskyi">
        <insert tableName="products">
            <column name="name" value="credit"/>
            <column name="type" value="CREDIT"/>
            <column name="status" value="AVAILABLE"/>
            <column name="currency_code" value="USD"/>
            <column name="interest_rate" value="8"/>
            <column name="product_limit" value="10000"/>
            <column name="created_at" value="2022-10-08"/>
            <column name="updated_at" value="2022-10-08"/>
        </insert>
    </changeSet>

</databaseChangeLog>
